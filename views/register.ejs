<% include layout %>
<div class="row">
	<div class="column side">
	</div>
	<div class="column middle">    
		<h1>Create Your Seller Account</h1>
			<form method='POST', action='register'>
				<h2>First Name</h2>
				<input name="firstName" type='text' placeholder='First Name'></input>
				<br>
				<h2>Last Name</h2>
				<input name="lastName" type='text' placeholder='Last Name'></input>
				<br>
				<h2>Email</h2>
				<input name="username" type='email' placeholder='Email'></input>
				<br>
				<h2>Password</h2>
				<input name="password" type='password' placeholder='Password'></input>
				<br>
				<h2>Confirm Password</h2>
				<input name="confirmpassword" type='password' placeholder='Confirm Password'></input>
				<br>
				<br>
				<br>
				<br>
				<button  type="submit" value="Save">Create Account</button>
			</form>	
			<br>
	</div>
</div>